{
  "hosting": {
    "public": "public",
    "rewrites": [ 
       {"source": "/helloWorld", "function": "helloWorld"}
      ,{"source": "/sample", "function": "sample"}
      ,{"source": "/create", "function": "create"}
      ,{"source": "/read", "function": "read"}
      ,{"source": "/update", "function": "update"}
      ,{"source": "/delete", "function": "delete"}
      ,{"source": "/appInfoInit", "function": "appInfoInit"}
      ,{"source": "/appInfoUpdate", "function": "appInfoUpdate"}
     ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
